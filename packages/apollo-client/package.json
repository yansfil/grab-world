{
  "name": "@grab-world/apollo-client",
  "version": "0.1.0",
  "scripts": {
    "dev": "NODE_ENV=src react-scripts start",
    "build": "react-scripts build",
    "codegen": "graphql-codegen --watch",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.12",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-batch-http": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-http": "^1.5.16",
    "apollo-link-persisted-queries": "^0.2.2",
    "apollo-link-retry": "^2.2.15",
    "apollo-link-ws": "^1.0.19",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-scripts": "3.2.0",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@apollo/react-components": "^3.1.3",
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/near-operation-file-preset": "^1.9.1",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-operations": "^1.9.1",
    "@graphql-codegen/typescript-react-apollo": "^1.9.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  }
}
